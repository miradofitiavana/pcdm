{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "pcdm/content-center",
  "version": "0.1.0",
  "title": "Content Center",
  "category": "pcdm",
  "icon": "smiley",
  "description": "Un bloc de contenu avec pre-title, titre, contenu",
  "supports": {
    "html": false
  },
  "textdomain": "content-center",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "keywords": [],
  "attributes": {
    "preTitle": {
      "type": "string",
      "source": "html",
      "selector": "p",
      "default": "Lorem ipsum dolor sit amet"
    },
    "contentTitle": {
      "type": "string",
      "source": "html",
      "selector": "h2",
      "default": "Lorem ipsum dolor sit amet"
    },
    "contentBody": {
      "type": "string",
      "selector": "div",
      "default": "<p>Lorem ipsum dolor sit amet consectetur adipisicing elit. Aperiam facilis, voluptates error alias dolorem praesentium sit soluta iure incidunt labore explicabo eaque, quia architecto veritatis dolores, enim cons equatur nihil ipsum.</p>"
    },
    "settings": {
      "type": "object",
      "default": {
        "showPreTitle": true
      }
    }
  }
}
